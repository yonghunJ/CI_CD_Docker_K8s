{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "lib":[ "ES2015", "ES2016", "ES2017", "ES2018", "ES2019", "ES2020"],
    "target": "ES2019",
    "strictNullChecks": true,
    "noImplicitAny": true,
    "strict": true,
    "strictFunctionTypes": false,
    "noImplicitThis": false,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "sourceMap": true,
    "outDir": "build",
    "rootDir": "src",  
    "baseUrl": ".",
  }
}